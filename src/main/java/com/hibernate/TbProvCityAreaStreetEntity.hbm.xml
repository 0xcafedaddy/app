<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.TbProvCityAreaStreetEntity" table="tb_prov_city_area_street" schema="xuexijia">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="pid">
            <column name="pid" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="level">
            <column name="level" sql-type="int(1)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>